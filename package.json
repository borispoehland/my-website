{
  "name": "boris_poehland_webseite",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS=--openssl-legacy-provider npm run sitemap:build && npm run sanity:build && npm run web:build",
    "start": "concurrently \"npm run sanity:start\" \"npm run web:start\"",
    "lint": "next lint",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,jsx}' 'pages/**/*.{ts,tsx,js,jsx}'",
    "husky:prepare": "husky install",
    "sanity:build": "cd cms && npx sanity build ../public/cms -y && cd ..",
    "sanity:start": "cpx .env.local cms/.env.development && cd cms && npx sanity start",
    "sitemap:build": "node sitemap.js",
    "web:build": "next build",
    "web:start": "next start",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {
    "@next/font": "^13.5.3",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sendgrid/mail": "^7.6.1",
    "axios": "^0.25.0",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "concurrently": "^7.0.0",
    "intersection-observer": "^0.12.0",
    "lerna": "^4.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "next": "^13.5.3",
    "next-sanity": "^0.4.0",
    "next-seo": "^5.1.0",
    "nextjs-sitemap-generator": "^1.3.1",
    "nprogress": "^0.2.0",
    "prettier": "^2.5.1",
    "puppeteer": "^13.3.1",
    "rate-limiter-flexible": "^2.3.6",
    "react": "^18.2.0",
    "react-bootstrap": "^2.1.2",
    "react-cool-inview": "^2.0.9",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.27.0",
    "react-icons": "^4.3.1",
    "react-moment": "^1.1.1",
    "react-scrollmagic": "^2.3.0",
    "react-syntax-highlighter": "^15.4.5",
    "react-uid": "^2.3.1",
    "recoil": "^0.6.1",
    "sass": "^1.49.7",
    "sharp": "^0.30.1",
    "sweetalert2": "^11.4.0",
    "use-memo-one": "^1.1.2"
  },
